# This simply creates 2 UDP sockets and sends a packet between them.

r0 = socket$inet_udp(AUTO, AUTO, AUTO)
r1 = syz_create_resource$ptr_sockaddr_in(&AUTO={AUTO, 0x4e20, @empty})
bind$inet(r0, r1, AUTO)
r2 = socket$inet_udp(AUTO, AUTO, AUTO)
sendto$inet(r2, &AUTO=""/10, AUTO, 0x0, &AUTO={AUTO, 0x4e20, @empty}, AUTO)
recvfrom(r0, &AUTO=""/10, AUTO, 0x0, 0x0, 0x0)
