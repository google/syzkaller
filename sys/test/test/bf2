syz_compare(&AUTO="1200000034860000", AUTO, &AUTO=@bf8={0x12, {0x34, 0x56, 0x78}}, AUTO)
syz_compare(&AUTO="12003486", AUTO, &AUTO=@bf9={0x12, {0x34, 0x56, 0x78}}, AUTO)
syz_compare(&AUTO="12000000000000004385670ad9bcf10e3402ab0000000000", AUTO, &AUTO=@bf10={0x12, {0x3, 0x4, 0x5, 0x678, 0x90a, 0xbcd, 0xef1, 0x234, 0xab}}, AUTO)
