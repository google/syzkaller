{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:allNonMajor",
        ":automergePatch",
        ":automergeMinor",
        ":automergeLinters",
        ":automergeTesters",
        ":automergeTypes"
    ],
    "enabled": true,
    "dependencyDashboard": false,
    "separateMajorMinor": true,
    "separateMultipleMajor": false,
    "prHourlyLimit": 2,
    "prConcurrentLimit": 10,
    "labels": [
        "depUpdate"
    ],
    "updateLockFiles": true,
    "docker": {
        "pinDigests": true
    }
}

